    "import pandas as pd\n",
    "\n",
    "def url_to_dataframe(url):\n",
    "    file_id = url.split('/')[-2]\n",
    "    download_url = 'https://drive.google.com/uc?id=' + file_id\n",
    "\n",
    "    data = pd.read_csv(download_url, encoding='utf-8', low_memory=False)\n",
    "    return data"
   ]
